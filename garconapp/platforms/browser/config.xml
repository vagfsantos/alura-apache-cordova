<?xml version='1.0' encoding='utf-8'?>
<widget id="vagner.santos.esy.es.garconapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BarcodeScanner">
        <param name="browser-package" value="BarcodeScanner" />
    </feature>
    <name>Garçonete só de cenoura</name>
    <description>
        App para uso das garçonetes dentro do restaurante
    </description>
    <author email="santos.vagnerferreira@gmail.com" href="http://vagnersantos.esy.es">
        Apache Cordova Team
    </author>
    <icon src="resources\android\icon\drawable-xhdpi-icon.png" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackgroundColor" value="0xF2F2F2FF" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
